{
  "licenseKey": "695aecfa-0329-41a2-b44f-c2e8fe28241d",
  "platform": {
    "uuid": "reactive-analytics-{{environment}}",
    "name": "Reactive Analytics{{environment_suffix}}",
    "icon": "{*host_url*}/static/media/reactive-analytics-icon-256x256.png",
    "defaultWindowOptions": {
      "contextMenu": true,
      "frame": true,
      "fdc3InteropApi": "1.2",
      "icon": "{*host_url*}/static/media/reactive-analytics-icon-256x256.png"
    },
    "defaultViewOptions": {
      "fdc3InteropApi": "1.2"
    }
  },
  "runtime": {
    "version": "<OPENFIN_RUNTIME>",
    "arguments": "--remote-debugging-port=9091"
  },
  "shortcut": {
    "company": "Adaptive Financial Consulting",
    "description": "Desktop Reactive Analytics",
    "icon": "{*host_url*}/static/media/reactive-analytics.ico",
    "name": "Reactive Analytics{{environment_suffix}}"
  },
  "snapshot": {
    "windows": [
      {
        "autoShow": true,
        "frame": true,
        "defaultWidth": 1280,
        "defaultHeight": 900,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "maximizable": true,
        "accelerator": {
          "devtools": true,
          "reload": true,
          "reloadIgnoringCache": true,
          "zoom": true
        },
        "url": "{*host_url*}",
        "name": "Desktop Reactive Analytics"
      }
    ]
  }
