{
  "platform": {
    "uuid": "reactive-analytics-{{environment}}",
    "name": "Reactive Analytics{{environment_suffix}}",
    "icon": "{*host_url*}/static/media/reactive-analytics-icon-256x256.png",
    "defaultWindowOptions": {
      "contextMenu": true,
      "frame": true,
      "icon": "{*host_url*}/static/media/reactive-analytics-icon-256x256.png"
    }
  },
  "runtime": {
    "version": "25.100.69.8"
  },
  "services": [
    {
      "name": "layouts"
    }
  ],
  "devtools_port": 9090,
  "shortcut": {
    "company": "Adaptive Consulting",
    "description": "Desktop Reactive Analytics",
    "icon": "{*host_url*}/static/media/reactive-analytics.ico",
    "name": "Reactive Analytics{{environment_suffix}}"
  },
  "snapshot": {
    "windows": [
      {
        "autoShow": true,
        "defaultWidth": 1280,
        "defaultHeight": 900,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "maximizable": true,
        "accelerator": {
          "devtools": true,
          "reload": true,
          "reloadIgnoringCache": true,
          "zoom": true
        },
        "url": "{*host_url*}",
        "name": "Desktop Reactive Analytics"
      }
    ]
  }
}
